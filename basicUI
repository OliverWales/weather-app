import javax.swing.*;
import java.awt.*;

class UI {

    public static void main(String[] args) throws Exception{
        JFrame f = new JFrame();
        f.setSize(1080/3,1920/3);

        GridLayout experimentLayout = new GridLayout(0,1);
        f.setLayout(experimentLayout);
        f.add(new JPanelWithBackgroundImage("src/01.jpg",1920/6, 1080/3));
        f.add(new JPanelWithBackgroundImage("src/02.jpg", 1920/6, 1080/3));
        f.setVisible(true);
    }

}
