class UI{
    
    public static void main(String[] args) throws Exception{
        JFrame f = new JFrame();
        // creates frame size of phone
        f.setSize(1080/3,1920/3);

        // Single column grid
        GridLayout experimentLayout = new GridLayout(0,1);
        f.setLayout(experimentLayout);
        
        // creates a JPanelWBI for each of the top & bottom
        f.add(new JPanelWithBackgroundImage("src/01.jpg"));
        f.add(new JPanelWithBackgroundImage("src/02.jpg"));
        
        // makes frame visible
        f.setVisible(true);
    }

}
